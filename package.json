{
  "name": "anki-pug",
  "version": "0.1.0",
  "description": "command that show and possibly merge diff between your pug model and html template",
  "bin": {
    "anki-pug": "bin/anki-pug.js",
    "fix-with-test": "bin/fix-with-test.js"
  },
  "main": "lib/index.js",
  "engines": {
    "node": ">=14"
  },
  "repository": "https+git@github.com:pulsovi/anki-cards.git",
  "author": "David GABISON <david.gabison@outlook.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "rm -rf ./bin && rm -rf ./lib && yarn babel src --out-dir . --extensions \".ts\"",
    "commit-msg": "yarn fix-with-test",
    "debug-tests": "yarn node --stack-trace-limit=100 --trace-warnings --async-stack-traces $(yarn bin jest)",
    "lint": "eslint .",
    "pre-commit": "yarn lint-staged",
    "prepack": "yarn build",
    "test": "jest"
  },
  "packageManager": "yarn@3.1.0",
  "dependencies": {
    "chalk": "^4.0.0",
    "chokidar": "^3.5.3",
    "commander": "^9.0.0",
    "core-js": "^3.21.1",
    "debug": "^4.3.3",
    "fs-extra": "^10.0.1",
    "joi": "^17.6.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "pkg-dir": "^5.0.0",
    "pug": "^3.0.2",
    "rc": "^1.2.8",
    "read-pkg-up": "^7.0.0",
    "readdirp": "^3.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.2",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@tsconfig/node14": "^1.0.1",
    "@types/babel__core": "^7",
    "@types/babel__preset-env": "^7",
    "@types/debug": "^4",
    "@types/eslint": "^8",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.4.0",
    "@types/js-yaml": "^4",
    "@types/lodash": "^4",
    "@types/node": "^17.0.17",
    "@types/pug": "^2.0.6",
    "@types/rc": "^1.2.1",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.8.0",
    "eslint-config-pulsovi-typescript": "^0.2.8",
    "jest": "^27.5.1",
    "jest-extended": "^1.1.0",
    "lint-staged": "^12.3.4",
    "typescript": "^4.5.5"
  }
}
